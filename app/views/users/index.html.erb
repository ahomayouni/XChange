	<%= form_with url: search_users_path, local: true, method: :get, html: {class: "form-inline"} do |form| %>
		<%= form.text_field :users_q, placeholder: "Search for a user..", data: { behaviour: "autocomplete_users" }, class: "form-control" %>
		<%= form.button "Search", class: "btn btn-outline-success" %>
	<% end %>		

	<%= provide(:title, 'All Users') %>
	<h1> All Users </h1>
	<%= will_paginate %>
		<div class="d-flex flex-row flex-wrap w-100">
			<%= render @users %>
		</div>
	<%= will_paginate %>

</div>

