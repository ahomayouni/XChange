<%= form_with url: search_listings_path, local: true, method: :get, html: {class: "form-inline"} do |form| %>
	<%= form.text_field :listings_q, placeholder: "Search for a listing..", data: { behaviour: "autocomplete_listings" }, class: "form-control" %>
	<%= form.button "Search", class: "btn btn-outline-success" %>
<% end %>	




<h1>Listing all Listings</h1>
<p>
<%= link_to "Create new listing", new_listing_path %>
</p>
<table>
 <tr>
  <th>Title</th>
  <th>Description</th>
 </tr>
 <% @listings.each do |listing| %>
 <tr>
  <td><%= listing.title %></td>
  <td><%= listing.description %></td>
  <td><%= listing.user.name %></td>
  <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
  <td><%= link_to 'Show', listing_path(listing) %></td>
  <td><%= link_to 'Delete', listing_path(listing), method: :delete, data: {confirm: "Are you sure?"} %></td>
 </tr>
 <% end %>
</table>
